<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.app.cn.dao.TravelInfoDao">
	<insert id="addTravelInfo" useGeneratedKeys="true"
		keyColumn="id" keyProperty="id">
		insert into
		tb_travel_info(username,to_where,start_time,people_num,process) values
		(#{username},#{toWhere},#{startTime},#{peopleNum},#{process})
	</insert>
</mapper>